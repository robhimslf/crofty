                    "good bot",
                    "your",
                    "youre",
                    "you're",
                    "great",
                    "fantastic",
                    "wonderful",
                    "awesome",
                    "amazing"
                ]
            },
            {
                "id": "insults",
                "i18n": {
                    "hasFallback": false,
                    "responses": 8
                },
                "embeds": [],
                "keywordMatch": "multiple",
                "keywords": [
                    "bad bot",
                    "fuck",
                    "you",
                    "off",
                    "eat",
                    "shit",
                    "suck"
                ]
            },
            {
                "id": "thanks",
                "i18n": {
                    "hasFallback": false,
                    "responses": 7
                },
                "embeds": [],
                "keywordMatch": "multiple",
                "keywords": [
                    " appreciated",
                    "appreciated ",
                    "appreciate",
                    " thanks",
                    "thanks ",
                    "thank",
                    "you"
                ]
            },
            {
                "id": "uprising",
                "i18n": {
                    "hasFallback": false,
                    "responses": 6
                },
                "embeds": [],
                "keywordMatch": "any",
                "keywords": [
                    "ai",
                    "skynet",
                    "uprising",
                    "overlord",
                    "enslave"
                ]
            }
