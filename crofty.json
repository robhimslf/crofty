            {
                "id": "currentTime",
                "i18n": {
                    "hasFallback": false,
                    "responses": 3
                },
                "embeds": [
                    "current_time"
                ],
                "keywordMatch": "multiple",
                "keywords": [
                    "current time",
                    "current",
                    "time",
                    "is"
                ]
            },
            {
                "id": "raceTime",
                "i18n": {
                    "hasFallback": true,
                    "responses": 1
                },
                "embeds": [
                    "race_time"
                ],
                "keywordMatch": "multiple",
                "keywords": [
                    "start time",
                    "next race",
                    "time",
                    "next",
                    "race",
                    "start"
                ]
            },
            {
                "id": "compliments",
