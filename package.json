{
    "name": "crofty",
    "version": "1.0.0",
    "description": "Crofty is a Formula 1 Discord bot written in Typescript. He provides historical statistics, automatic race event thread creation, and more.",
    "homepage": "https://github.com/robhimslf/crofty#readme",
    "repository": "https://github.com/robhimslf/crofty.git",
    "bugs": {
        "url": "https://github.com/robhimslf/crofty/issues"
    },
    "author": "Rob Himself <rob@robhimself.com>",
    "license": "MIT",
    "type": "module",
    "main": "build/crofty.js",
    "scripts": {
        "postinstall": "tsc --project tsconfig.json",
        "dev": "node --loader ts-node/esm/transpile-only src/crofty.ts",
        "serve": "node build/crofty.js",
        "start-node": "nodemon --exec node --loader ts-node/esm/transpile-only src/crofty.ts",
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
    },
    "engines": {
        "node": "16.13.0",
        "npm": "8.1.0",
        "yarn": "1.22.15"
    },
    "dependencies": {
        "@discordx/importer": "^1.1.9",
        "@discordx/pagination": "^2.2.0",
        "@discordx/utilities": "^4.3.1",
        "@types/i18next-fs-backend": "^1.1.2",
        "@types/jest": "^27.5.1",
        "@types/luxon": "^2.3.2",
        "@types/node": "^17.0.36",
        "@types/node-cron": "^3.0.1",
        "@types/node-schedule": "^2.1.0",
        "@types/xml2js": "^0.4.11",
        "cheerio": "^1.0.0-rc.11",
        "cloudinary": "^1.30.0",
        "discord.js": "^13.7.0",
        "discordx": "^9.7.1",
        "dotenv": "^16.0.1",
        "firebase-admin": "^10.2.0",
        "i18next": "^21.8.9",
        "i18next-fs-backend": "^1.1.4",
        "luxon": "^2.4.0",
        "jest": "^28.1.0",
        "node-cron": "^3.0.0",
        "node-fetch": "^3.2.4",
        "node-schedule": "^2.1.0",
        "nodemon": "^2.0.16",
        "prettier": "2.6.2",
        "reflect-metadata": "^0.1.13",
        "rss-parser": "^3.12.0",
        "ts-jest": "^28.0.3",
        "ts-node": "^10.8.0",
        "typescript": "^4.7.2"
    }
}
